{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": "vite",
    "outputDirectory": "dist",
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
  